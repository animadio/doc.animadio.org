<article class="menu-target3">
    <h3>Place</h3>

    <table class="table">
        <caption>Place</caption>

        <thead>
        <tr>
            <th>Class</th>
            <th>Property</th>
            <th>Variable</th>
            <th>Media</th>
            <th>Concat</th>
            <th>State</th>
        </tr>
        </thead>

        <tbody>
        {% for place in allPlaceClasses %}
            <tr>
                <th>{{ place.class }}</th>
                <td>{{ place.property }}</td>
                <td>{{ place.variable }}</td>
                <td>{{ place.media|replace({'1': "yes", '0': "no"}) }}</td>
                <td>{{ place.concat|replace({'1': "yes", '0': "no"}) }}</td>
                <td>{{ place.state|replace({'1': "yes", '0': "no"}) }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</article>
